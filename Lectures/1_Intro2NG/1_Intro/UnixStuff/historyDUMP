#!/bin/bash

DIR=$1
ls -1 $DIR/history_* | xargs cat | sort | uniq | fgrep -v "#==(" >_TMP
rm $DIR/history_*
mv _TMP $DIR/history_COMPRESS
BAM=../../Cache/dmpshare/dmp/dms/processed-data/bam/2014/AB8314174803EB/HR098283-N.bam
bedtools bamtobed
bedtools bamtobed -g
bedtools bamtobed -i $BAM 
bedtools bamtobed -i $BAM | bedtools merge -i -
bedtools bamtobed -i $BAM | bedtools merge -i - >test.bed
bedtools bamtobed -i test.bam 
bedtools bamtobed -i test.bam | bedtools merge -i -
bedtools bamtobed -i test.bam | bedtools merge -i - | bedtools coverage -b - -abam test.bam 
bedtools bamtobed -i test.bam | bedtools merge -i - | bedtools coverage -b - -abam test.bam | awk -v COV '$4>=COV{print $0}'
bedtools bamtobed -i test.bam | bedtools merge -i - | bedtools coverage -b - -abam test.bam | tee cov
bedtools coverage
bedtools coverage 
bedtools coverage -bed test.bed 
bedtools coverage -bed test.bed -bam $BAM
bedtools coverage -b test.bed -abam $BAM
bedtools coverage -b test.bed -abam $BAM | awk '$4>=10{print $1,$2,$3}' | tr ' ' '\t'  | tee cov
bedtools coverage -b test.bed -abam $BAM | awk '$4>=10{print $1,$2,$3}' | tr ' ' '\t'  | wc -l
bedtools coverage -b test.bed -abam $BAM | tee cov
bg
bjobs
bjobs | more
bjobs | wc
bkill -b 0
bsub -We 10 -J MK ./mkCoveredTargets.sh 20 ../../Cache/dmpshare/dmp/dms/processed-data/bam/2015/WG9423781711RK/OH895947-T.bam out/OH895947-T.bam/OH895947-T.bam.bed
c
cat >bam2bedMerge.sh
cat >CMDS
cat >>CMDS 
cat cov | awk '$4>10{print $0}'
cat cov | awk -v COV '$4>10{print $0}'
cat cov | awk -v COV=COV '$4>10{print $0}'
cat cov | awk -v COV=$COV '$4>COV{print $0}'
cat cov | awk -v COV=COV '$4>COV{print $0}'
cat /dev/stdout 
cat merge.bed | awk '{print $3-$2}'
cat merge.bed | awk '{print s+=$3-$2}'
cat merge.bed out/HT654730-T.bam/HT654730-T.bam.bed | bedtools merge -i - | wc -l
cat merge.bed out/HT654730-T.bam/HT654730-T.bam.bed | bedtools sort -i - | bedtools merge -i - >join.bed
cat merge.bed out/HT654730-T.bam/HT654730-T.bam.bed | bedtools sort -i - | bedtools merge -i - | wc -l
cat merge.bed out/HT654730-T.bam/HT654730-T.bam.bed | wc -l
cat merge.bed | wc -l
cat out/Proj_5304_C_indelRealigned_recal_s_MM02_10.bam/Proj_5304_C_indelRealigned_recal_s_MM02_10.bam.bed
cat out/Proj_5304_C_indelRealigned_recal_s_MM02_10.bam/Proj_5304_C_indelRealigned_recal_s_MM02_10.bam.bed | awk '{print s+=$3-$2}'
cat out/Proj_5304_C_indelRealigned_recal_s_MM02_10.bam/Proj_5304_C_indelRealigned_recal_s_MM02_10.bam.bed | more
cd
cd ..
cd /home/socci/Work/SingerS/CragoA/150804_PaperDatasets/Set5_MAF
cd /ifs/res/socci/Cache/Kentsis/TargetWholeGenomes/TargetRhabdoidTumors/Project_KentsisWGS_RTumors/
chmod u+x mkCoveredTargets.sh 
~/Code/Pipelines/CBE/Variant/PostProcess/postProcess.sh 
~/Code/Pipelines/CBE/Variant/PostProcess/postProcess.sh Proj_5717_C_r1_sample_pairing.txt /ifs/res/seq/soccin/soccin/Proj_5717_C_r1/r_002 
COV=10
cp /ifs/projects/BIC/Proj_5717_C_r1/Proj_5717_C_r1_sample_pairing.txt .
cp merge.bed abra_target_regions_CustomeExome_hg19.bed
cp out/MY545782-T.bam/MY545782-T.bam.bed merge.bed
echo $COV
find out/
find out -type f
find out/ -type f
find out -type f | xargs -n 1 ls -s
find out -type f | xargs wc -l
find out -type f | xargs wc -l | awk '$1>0{print $0}'
find out -type f | xargs wc -l | awk '$1>0{print $0}' | fgrep bed
find out -type f | xargs wc -l | awk '$1>0{print $0}' | fgrep bed | awk '{print $1}'
find out -type f | xargs wc -l | awk '$1>0{print $0}' | fgrep bed | awk '{print $2}'
find out -type f | xargs wc -l | awk '$1>0{print $0}' | fgrep bed | awk '{print $2}' | head 
for file in $(find ../../Cache -name '*.bam'); do base=$(basename $file); echo $base; done
for file in $(find ../../Cache -name '*.bam'); do base=$(basename $file); mkdir -p out/$base; bsub -We 10 -J MK -n 4 ./mkCoveredTargets.sh 20 $file out/$base/$base.bed; done
for file in $(find ../../Cache -name '*.bam'); do base=$(basename $file); mkdir -p out/$base;  echo bsub -We 10 -J MK ./mkCoveredTargets.sh 20 $file out/$base/$base.bed; done
for file in $(find ../../Cache -name '*.bam'); do echo $file; done
for file in $(find /ifs/res/seq/componc/soccin/Proj_5304_C/r_001/alignments -name '*.bam'); do base=$(basename $file); echo $base; done
for file in $(find /ifs/res/seq/componc/soccin/Proj_5304_C/r_001/alignments -name '*.bam'); do base=$(basename $file); echo $file; bsub -o LSF/ -We 10 -J MK -n 4 ./mkCoveredTargets.sh 20 $file out/$base/$base.bed; done
for file in $(find /ifs/res/seq/componc/soccin/Proj_5304_C/r_001/alignments -name '*.bam'); do base=$(basename $file); echo $file;  bsub -We 10 -J MK -n 4 ./mkCoveredTargets.sh 20 $file out/$base/$base.bed; done
for file in $(find /ifs/res/seq/componc/soccin/Proj_5304_C/r_001/alignments -name '*.bam'); do base=$(basename $file); mkdir -p out/$base; bsub -o LSF/ -We 10 -J MK -n 4 ./mkCoveredTargets.sh 20 $file out/$base/$base.bed; done
for file in $(find out -type f | xargs wc -l | awk '$1>0{print $0}' | fgrep bed | awk '{print $2}'); do echo $file; cat merge.bed $file | bedtools sort -i - | bedtools merge -i - >join.bed; mv join.bed merge.bed ; done
for file in $(find out -type f | xargs wc -l | awk '$1>0{print $0}' | fgrep bed | awk '{print $2}'); do echo $file; done
for file in $(find out -type f | xargs wc -l | awk '$1>0{print $0}' | fgrep bed | awk '{print $2}' | head); do echo $file; cat merge.bed $file | bedtools sort -i - | bedtools merge -i - >join.bed; mv join.bed merge.bed ; done
history
history | fgrep bedtools
history | fgrep bsub
history | fgrep merge
history | fgrpe bedtools
ls
ls -1
lsa
ls -asl
ls ../../Cache/dmpshare/dmp/dms/processed-data/bam/201*/*/*bam
ls ~/Code/Pipelines/CBE/Variant/variants_pipeline/targets/abra/
ls -dlrt /ifs/res/seq/componc/soccin/Proj_5*
ls -d out/*-N.bam
ls -d out/*-N.bam | xargs -t rm -rf 
ls -d out/*-T.bam | xargs -t rm -rf 
ls -lrt
ls -lrt /ifs/res/seq/componc/soccin/Proj_5*
ls LSF/1059*
ls out/
ls out/*-N.bam
ls out/Proj_5304_C_indelRealigned_recal_s_MM0*
ls out/Proj_5304_C_indelRealigned_recal_s_MM0*/*bed
ls out/Proj_5304_C_indelRealigned_recal_s_MM0*/*bed | xargs wc
ls out/Proj_5304_C_indelRealigned_recal_s_MM0*/*bed | xargs wc -
ls out/Proj_5304_C_indelRealigned_recal_s_MM0*/*bed | xargs wc -l
man awk
md5sum GV686798-N.bam.safe | tee -a MD5 
md5sum GV686798-N.bam | tee MD5
./mkCoveredTargets.sh 50 test.bam 
./mkCoveredTargets.sh 50 test.bam /dev/stdout 
./mkCoveredTargets.sh 50 test.bam test.cov
./mkCoveredTargets.sh 50 test.bam test.cov 
./mkCoveredTargets.sh 50 test.bam test.cov /dev/stdout 
./mkCoveredTargets.sh test.bam 
./mkCoveredTargets.sh test.bam | wc -l
more abra_target_regions_CustomeExome_hg19.bed 
more bam2bedMerge.sh 
more CMDS 
more cov
more cov | awk '$4>20{print $1,$2,$3}'
more cov | awk '$4>20{print $1,$2,$3}' | wc
more GV686798-N.bam.fix.sam 
more LSF/*
more LSF/10598*
more LSF/* | fgrep Max
more MD5 
more merge.bed 
more merge_maf3.pairs.tsv 
more merge_maf3.vcf 
more mkCoveredTargets.sh 
more out/OH895947-T.bam/OH895947-T.bam.bed 
more out/OH895947-T.bam/OH895947-T.bam.bed | wc -l
more  out/TU034107-N.bam/TU034107-N.bam.bed
more  out/TU034107-N.bam/TU034107-N.bam.bed | wc -l
more Proj_5717_C_r1_sample_pairing.txt 
more Proj_5717_C_r1_TEST___SOMATIC.vep.maf 
more test.bed 
mr -rf LSF/ out/
mv merge.bed brca_Prostate.bed
picard.local DownsampleSam
picard.local DownSampleSam
